{
  "name": "getx-light-bulb",
  "displayName": "GetX Light Bulb",
  "description": "CMD + . to show shortcut to wrap body for Getx",
  "version": "0.2.0",
  "publisher": "HyLun",
  "author": "HyLun",
  "repository": {
    "type": "git",
    "url": "https://github.com/haolun1996/GetX-Light-Bulb.git"
  },
  "keywords": [
    "dart",
    "flutter",
    "get",
    "getX",
    "state-management",
    "Obx",
    "Shortcut"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters",
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:pubspec.yaml",
    "workspaceContains:*/pubspec.yaml",
    "workspaceContains:*/*/pubspec.yaml",
    "workspaceContains:*.dart",
    "workspaceContains:*/*.dart",
    "workspaceContains:*/*/*.dart"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "getx-light-bulb.wrapObx",
        "title": "GetX Obx Wrap",
        "category": "BaseX"
      },
      {
        "command": "getx-light-bulb.removeObx",
        "title": "Remove this Obx",
        "category": "BaseX"
      },
      {
        "command": "getx-light-bulb.expanded",
        "title": "Expanded",
        "category": "BaseX"
      },
      {
        "command": "getx-light-bulb.getXCreateFile",
        "title": "BaseX: Create New Page",
        "category": "BaseX"
      }
    ],
    "snippets": [
      {
        "language": "dart",
        "path": "./snippets/flutter.json"
      }
    ],
    "menus": {
      "_comment": [
        {
          "explorer/context": [
            {
              "when": "explorerResourceIsFolder",
              "command": "getx-light-bulb.getXCreateFile",
              "group": "1_basex@1"
            }
          ]
        }
      ],
      "commandPalette": [
        {
          "command": "getx-light-bulb.wrapObx",
          "when": "editorLangId == dart"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1"
  }
}